    "outDir": "./dist",
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": false,
    "jsx": "react",
    "declaration": true
  },
  "include": [
    "./src/*.tsx",
    "./src/*.ts",
    "./src/*.less"
  ],
  "exclude": [ "node_modules", "typings" ]
}